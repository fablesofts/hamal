package com.fable.hamal.shuttle.common.model.envelope.et.filter;

import java.io.Serializable;
import java.util.List;

/**
 * "columnFilter":[{"name":"id_num","operator":"contain|equal|!greater|smaller|!regexp|fixlength","value":["1"]}]
 * @author xieruidong 2013年12月23日 下午4:42:14
 */
public class ColumnFilter implements Serializable {

	private static final long serialVersionUID = 4095239345285099446L;

	private String name;
	
	private String operator;
	
	private String keyword;
	
	private List<String> value;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public List<String> getValue() {
		return value;
	}

	public void setValue(List<String> value) {
		this.value = value;
	}

    
    public String getKeyword() {
        return keyword;
    }

    
    public void setKeyword(String keyword) {
        this.keyword = keyword;
    }

    
    public static long getSerialversionuid() {
        return serialVersionUID;
    }
	
	
}
