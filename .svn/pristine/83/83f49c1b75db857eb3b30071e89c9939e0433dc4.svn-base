package com.fable.hamal.manager.exception;

/**
 * 
 * @author xieruidong 2014年1月16日 下午3:03:50
 */
public class ConfigRuntimeException extends RuntimeException {

	private static final long serialVersionUID = -7537063157630024725L;

	public ConfigRuntimeException() {
		super();
	}
	
	public ConfigRuntimeException(String msg) {
		super(msg);
	}
	
	public ConfigRuntimeException(String msg, Throwable cause) {
		super(msg, cause);
	}
	
	public ConfigRuntimeException(Throwable cause) {
		super(cause);
	}
}
