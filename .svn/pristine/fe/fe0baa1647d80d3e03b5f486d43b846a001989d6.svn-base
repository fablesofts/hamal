package com.fable.hamal.shuttle.common.data.media;

import java.io.Serializable;

/**
 * 
 * @author xieruidong 2013年11月12日 下午2:40:24
 */
public class DataCarrierSource implements Serializable {

	private static final long serialVersionUID = -2327936070412830444L;

	private Long id;
	private String name;
	private DataCarrierType type;
	private String encode = "utf8";
	
	
	public Long getId() {
		return id;
	}
	
	public void setId(Long id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public DataCarrierType getType() {
		return type;
	}
	
	public void setType(DataCarrierType type) {
		this.type = type;
	}
	
	public String getEncode() {
		return encode;
	}
	
	public void setEncode(String encode) {
		this.encode = encode;
	}
}
